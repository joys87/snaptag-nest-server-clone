{
  "name": "snaptag-node-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "cross-env NODE_ENV=local nest start",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "cross-env NODE_ENV=debug nest start --debug --watch",
    "start:prod": "nest build & cross-env NODE_ENV=prod node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:push:local": "dotenv -e environment/.local.env -- prisma db push",
    "prisma:migrate:create-only:dev": "dotenv -e environment/.dev.env -- npx prisma migrate dev --create-only",
    "prisma:migrate:dev": "dotenv -e environment/.dev.env -- npx prisma migrate dev",
    "prisma:migrate:reset:dev": "dotenv -e environment/.dev.env -- npx prisma migrate reset",
    "prisma:migrate:deploy": "dotenv -e environment/.prod.env -- npx prisma migrate deploy && prisma generate",
    "prisma:seed:local": "dotenv -e environment/.local.env -- npx prisma db seed",
    "prisma:seed:dev": "dotenv -e environment/.dev.env -- npx prisma db seed",
    "prisma:seed:test-user:dev": "dotenv -e environment/.dev.env -- ts-node prisma/seeding/seed-tests.ts",
    "prisma:reset-sequence:local": "dotenv -e environment/.local.env ts-node prisma/reset-internal-sequence.ts",
    "prisma:reset-sequence:dev": "dotenv -e environment/.dev.env ts-node prisma/reset-internal-sequence.ts",
    "prisma:reset-sequence:prod": "dotenv -e environment/.prod.env ts-node prisma/reset-internal-sequence.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/bull": "^0.6.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/throttler": "^2.0.1",
    "@prisma/client": "^3.15.1",
    "@sentry/node": "^7.12.1",
    "@slack/webhook": "^6.1.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/multer": "^1.4.7",
    "aws-sdk": "^2.1174.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.8.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "dotenv-cli": "^5.1.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "express": "^4.17.3",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prisma": "^3.15.1",
    "prisma-erd-generator": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.30.7",
    "shelljs": "^0.8.5",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bull": "^3.15.9",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^18.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/lib(|/.*)$": "<rootDir>/libs/lib/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/utils(|/.*)$": "<rootDir>/libs/utils/src/$1",
      "^@app/jwt(|/.*)$": "<rootDir>/libs/jwt/src/$1",
      "^@app/cookie(|/.*)$": "<rootDir>/libs/cookie/src/$1",
      "^@app/upload-image(|/.*)$": "<rootDir>/libs/upload-image/src/$1",
      "^@app/worker-server(|/.*)$": "<rootDir>/libs/worker-server/src/$1",
      "^@app/iamport(|/.*)$": "<rootDir>/libs/iamport/src/$1"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seeding/seed.ts"
  }
}
